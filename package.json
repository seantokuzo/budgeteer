{
  "name": "budgeteer",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:client": "cd client && npm run build",
    "build:server": "tsc",
    "build": "concurrently --kill-others -p \"[{name}]\" -n \"server,client\" -c \"bgRed.bold,white.bgBlue.bold\" \"npm run build:server\" \"npm run build:client\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "NODE_ENV=development nodemon",
    "dev": "concurrently --kill-others -p \"[{name}]\" -n \"server,client\" -c \"bgRed.bold,white.bgBlue.bold\" \"npm run dev:server\" \"npm run dev:client\"",
    "lint": "bash ./scripts/lint.sh",
    "lint:trace": "npx cross-env NODE_OPTIONS=--trace-deprecation npm run lint",
    "lint:fix": "bash ./scripts/lint.sh --fix",
    "test-client": "cd client && npm test",
    "test-client:watch": "cd client && npm run test:watch",
    "test-client:silent": "cd client && npm run test:silent",
    "test-client:mem": "cd client && npm run test-mem",
    "test-client:update-snapshot:all": "cd client && npm run test:update-snapshot:all",
    "test-client:update-snapshot:one": "cd client && npm run test:update-snapshot:one",
    "test-server": "jest --verbose --colors --detectOpenHandles",
    "test-server:watch": "jest --verbose --colors --detectOpenHandles --watch",
    "test-server:silent": "jest --colors --detectOpenHandles --silent",
    "test-server:mem": "node --expose-gc ./node_modules/.bin/jest -w=1 --logHeapUsage --verbose --colors",
    "test": "concurrently -p \"{name}>\" -n \"server,client\" -c \"red.bold,white.blue.bold\" \"npm run test-server\" \"npm run test-client\"",
    "test:mem": "concurrently -p \"{name}>\" -n \"server,client\" -c \"red.bold,white.blue.bold\" \"npm run test-server:mem\" \"npm run test-client:mem\"",
    "test:local": "concurrently -p \"{name}>\" -n \"server,client\" -c \"red.bold,white.blue.bold\" \"npm run test-server:local\" \"npm run test-client\"",
    "test:silent": "concurrently -p \"{name}>\" -n \"server,client\" -c \"red.bold,white.blue.bold\" \"npm run test-server:silent\" \"npm run test-client:silent\"",
    "docker-build-dev": "docker build -f Dockerfile-dev -t seantokuzo/budgeteer-dev .",
    "docker-push-dev": "docker push seantokuzo/budgeteer-dev",
    "docker-dev": "docker compose -f docker-compose-dev.yml up",
    "docker-lint": "LINT_COMMAND=lint docker compose -f docker-compose-lint.yml up --abort-on-container-exit",
    "docker-lint:trace": "LINT_COMMAND=lint:trace docker compose -f docker-compose-lint.yml up --abort-on-container-exit",
    "docker-lint:fix": "LINT_COMMAND=lint:fix docker compose -f docker-compose-lint.yml up --abort-on-container-exit",
    "docker-test": "TEST_COMMAND=test docker compose -f docker-compose-test.yml up --abort-on-container-exit",
    "docker-test:client": "TEST_COMMAND=test-client docker compose -f docker-compose-test.yml up --abort-on-container-exit",
    "docker-test:server": "TEST_COMMAND=test-server docker compose -f docker-compose-test.yml up --abort-on-container-exit",
    "docker-test:silent": "TEST_COMMAND=test:silent docker compose -f docker-compose-test.yml up --abort-on-container-exit",
    "docker-test:solo": "bash ./scripts/test-solo.sh",
    "docker-test:solo-watch": "bash ./scripts/test-solo-watch.sh",
    "dtsw": "bash ./scripts/test-solo-watch.sh",
    "docker-test:update-snapshot:all": "TEST_COMMAND=test-client:update-snapshot:all docker compose -f docker-compose-test.yml up --abort-on-container-exit",
    "docker-test:update-snapshot:one": "bash ./scripts/test-update-snapshot-solo.sh",
    "docker-build-check": "bash ./scripts/build-check.sh",
    "docker-remove-all": "bash ./scripts/docker-remove-all.sh budgeteer budgeteer budgeteer"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.0.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-react": "^7.35.2",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.4.0"
  }
}
