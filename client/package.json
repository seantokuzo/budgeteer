{
  "name": "budgeteer-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite",
    "build": "tsc && vite build",
    "test": "jest --verbose --colors",
    "test:update-snapshot:all": "jest --verbose --colors --updateSnapshot",
    "test:update-snapshot:one": "jest --verbose --colors --updateSnapshot --testNamePattern",
    "test:watch": "jest --verbose --colors --watch",
    "test:silent": "jest --colors --detectOpenHandles  --silent",
    "test-mem": "node --expose-gc ./node_modules/.bin/jest -w=1 --logHeapUsage --verbose --colors"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.4.1"
  }
}
